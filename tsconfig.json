{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "noEmitOnError": false,
    "rootDir": ".",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": false,
    "sourceRoot": "",
    "inlineSourceMap": false,
    "inlineSources": false
  },
  "filesGlob": [
    "**/*.ts",
    "**/*.tsx",
    "!node_modules/**"
  ],
  "files": [
    "app/app.settings.ts",
    "app/app.ts",
    "app/helper/observableFirebase.ts",
    "app/model/order_item.ts",
    "app/model/order.ts",
    "app/model/Serializable.ts",
    "app/model/service_result.ts",
    "app/model/user.ts",
    "app/pages/dashboard/dashboard.ts",
    "app/pages/home/home.ts",
    "app/pages/login/login.service.ts",
    "app/pages/login/login.ts",
    "app/pages/orders/orders.service.ts",
    "app/pages/orders/orders.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": false,
  "atom": {
    "rewriteTsconfig": true
  }
}
